@model MyWeddingPlanner.Web.ViewModels.Vendors.VendorListViewModel
@{
    this.ViewData["Title"] = "All Vendors";
}

<h1>@this.ViewData["Title"]</h1>
<hr />
<div class="row">
    @foreach (var vendor in this.Model.Vendors)
    {
        <div class="media col-md-4">
            <img src="https://res.cloudinary.com/ufn/image/upload/c_pad,f_auto,fl_progressive,h_500,w_445/u7cdzxvxu69pmubmtltc.jpg" width="100" class="mr-3 img-responsive">
            <div class="media-body">
                <p>@vendor.Name</p>
                <p>@vendor.Description</p>
                <p>@vendor.PhoneNumber</p>
            </div>
        </div>
    }
</div>
<hr />